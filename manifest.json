{
  "name": "EdStem Fixer",
  "version": "0.0.1",
  "manifest_version": 3,
  "description": "Makes the Lessons page on EdStem more readable.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "content_scripts": [
    {
      "matches": [
        "https://edstem.org/us/courses/*/lessons/"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}